{
  "name": "@ederzeel/json-schema-yup-transformer",
  "author": "Liam Ederzeel",
  "version": "0.1.0-alpha.8",
  "type": "module",
  "description": "Transforms a draft 7 specification JSON Schema to a Yup Schema",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liamederzeel/json-schema-yup-transform"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./run": "./dist/run.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.mts",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "build": "tsdown",
    "lint": "oxlint --type-aware",
    "test": "vitest",
    "test:debug": "DEBUG='JsonSchemaYupTransform::Builder::ConditionalSchema' vitest",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "debug": "^4.4.3",
    "is-relative-url": "4.1.0",
    "lodash-es": "^4.17.22",
    "stringify-object": "^3.2.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/json-schema": "^7.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/stringify-object": "^3.2.0",
    "coveralls": "^3.0.11",
    "oxlint": "^1.33.0",
    "oxlint-tsgolint": "^0.9.1",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "keywords": [],
  "packageManager": "pnpm@10.20.0"
}
